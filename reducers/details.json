{
  "root": {
    "path": "",
    "on_null": "return_empty"
  },
  "pipeline": [
    {
      "op": "copy",
      "from": "alerts[].intelAgentsGrouped",
      "to": "reduced_results.alerts_intel_agents"
    },
    {
      "op": "copy",
      "from": "alerts[].metadata",
      "to": "reduced_results.alerts_metadata"
    },
    {
      "op": "copy",
      "from": "alerts[].linkedAlerts",
      "to": "reduced_results.alerts_linked_alerts"
    },
    {
      "op": "prune",
      "strategy": "empty_values",
      "target": "reduced_results"
    }
  ],
  "output": {
    "result": "$working.reduced_results"
  }
}
